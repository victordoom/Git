@using Microsoft.AspNetCore.Identity
@using WebAdmin.Areas.Identity.Data
@inject SignInManager<WebAdminUser> SignInManager
@inject UserManager<WebAdminUser> UserManager

@if (SignInManager.IsSignedIn(User))
{
    <form asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/Index", new { area = "" })" method="post" id="logoutForm">
        
                <button type="submit" class="btn btn-default btn-flat">Sign out</button>
            
    </form>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        @*<li><a asp-area="Identity" asp-page="/Account/Register">Register</a></li>*@
        <li><a asp-area="Identity" asp-page="/Account/Login">Login</a></li>
    </ul>
}
